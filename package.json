{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/eslint-plugin-shuunen/issues",
  "dependencies": {
    "@typescript-eslint/utils": "7.13"
  },
  "description": "My personal ESLint configs & rules",
  "devDependencies": {
    "@eslint/js": "9.5",
    "@types/eslint__js": "8.42",
    "@types/node": "20",
    "dependency-cruiser": "latest",
    "esbuild": "0.21",
    "eslint": "9.5",
    "eslint-plugin-unicorn": "54",
    "globals": "15",
    "repo-check": "1.35",
    "ts-node": "10",
    "typescript": "5.4",
    "typescript-eslint": "7.13",
    "watchlist": "0"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/Shuunen/eslint-plugin-shuunen",
  "keywords": [
    "eslint",
    "plugin",
    "shuunen"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "name": "eslint-plugin-shuunen",
  "peerDependencies": {
    "eslint": "^9.5",
    "typescript": "*"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/eslint-plugin-shuunen.git"
  },
  "scripts": {
    "build": "echo no build task is needed && echo build success",
    "check": "pnpm install && echo install success && pnpm build && repo-check && pnpm lint && pnpm test && echo check success",
    "lint": "pnpm lint:tsc && pnpm lint:biome && pnpm lint:eslint && pnpm lint:dep && echo lint success",
    "lint:biome": "echo no biome yet && echo lint:biome success",
    "lint:dep": "depcruise src --config && echo lint:dep success",
    "lint:eslint": "eslint --cache --max-warnings 0 src && echo lint:eslint success",
    "lint:tsc": "tsc --noEmit && echo lint:tsc success",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm ci",
    "start": "pnpm dev",
    "test": "echo rework tests && echo test success",
    "test:watch": "watchlist src -eager -- npm test"
  },
  "type": "module",
  "typings": "src/index.ts",
  "version": "0.0.0"
}